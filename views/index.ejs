<%- include('./partials/head') %>

  <main>

    <h1 class="titel">
      <%= squad.name %>
    </h1>

    <section class="overzichtspagina">

      <!-- Loopt door alle klasgenoten uit de database -->
      <% squad.members.forEach(member=> { %>


        <article class="jou-lengte">

          <div class="liniaal"></div>

          <ul>
            <li>200cm -</li>
            <li>190cm -</li>
            <li>180cm -</li>
            <li>170cm -</li>
            <li>160cm -</li>
            <li>150cm -</li>
          </ul>

          <div class="context">
            <!-- Loopt door al de avatars van de klasgenoten uit de database -->
            <% if (member.avatar) { %>
              <img class="avatar" id="avatar" src="<%= member.avatar %>" alt="De avatar van <%= member.name %>" />
              <% }else{ %>
                <img class="avatar" src="/placeholder.jpg" alt="Placeholder avatar" />
                <% } %>

          </div>
        </article>

        <% }) %>

    </section>

  </main>

  <%- include('./partials/foot') %>